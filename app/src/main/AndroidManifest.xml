        <activity
            android:name=".mvvm.MvvmAct"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustPan"></activity>
        <activity
            android:name=".mvp.mvpactivity.MvpActivity"
            android:launchMode="singleTask"
